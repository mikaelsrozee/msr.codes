{
  "name": "msr.codes",
  "version": "1.0.0",
  "description": "Personal blog website, using Strapi backend and Next.js frontend",
  "main": "app.js",
  "scripts": {
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-backend": "cd backend && npm run start",
    "start-frontend": "cd frontend && npm run start",
    "dev": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\"",
    "dev-backend": "cd backend && npm run dev",
    "dev-frontend": "cd frontend && npm run dev",
    "setup": "concurrently \"npm run setup-backend\" \"npm run setup-frontend\"",
    "setup-backend": "",
    "setup-frontend": "cd frontend && next build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikaelsrozee/msr.codes.git"
  },
  "author": "Mikael Rozee",
  "bugs": {
    "url": "https://github.com/mikaelsrozee/msr.codes/issues"
  },
  "homepage": "https://github.com/mikaelsrozee/msr.codes#readme",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "concurrently": "^5.3.0",
    "knex": "<0.20.0",
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass": "^1.26.10",
    "sqlite3": "latest",
    "strapi": "3.1.4",
    "strapi-admin": "3.1.4",
    "strapi-connector-bookshelf": "3.1.4",
    "strapi-plugin-content-manager": "3.1.4",
    "strapi-plugin-content-type-builder": "3.1.4",
    "strapi-plugin-email": "3.1.4",
    "strapi-plugin-graphql": "3.1.4",
    "strapi-plugin-upload": "3.1.4",
    "strapi-plugin-users-permissions": "3.1.4",
    "strapi-utils": "3.1.4",
    "tailwindcss": "^1.8.4"
  }
}
